#!/usr/bin/env bash

# https://github.com/caskroom/homebrew-fonts/tree/master/Casks
# To support machines with multiple accounts the fonts need to be REINSTALLED
# per user account
brew cask reinstall font-m-plus
brew cask reinstall font-clear-sans
brew cask reinstall font-roboto
brew cask reinstall font-dejavu-sans
brew cask reinstall font-dejavu-sans-mono-for-powerline
brew cask reinstall font-inconsolata-dz
brew cask reinstall font-inconsolata-dz-for-powerline

